<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable name="itemLocationCitySearchModel"
                  type="com.slash.youth.ui.viewmodel.ItemLocationCitySearchModel"/>
    </data>
    <LinearLayout
        android:id="@+id/ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/tv_search_city_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="21.5dp"
            android:layout_marginLeft="11dp"
            android:layout_marginTop="21.5dp"
            android:text="@{itemLocationCitySearchModel.searchCityName}"
            android:textColor="#333333"
            android:textSize="16.5sp"/>
    </LinearLayout>
</layout>

