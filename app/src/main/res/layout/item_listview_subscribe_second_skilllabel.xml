<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable name="ttemSubscribeSecondSkilllabelModel"
                  type="com.slash.youth.ui.viewmodel.ItemSubscribeSecondSkilllabelModel"/>
    </data>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="58dp"
        >
            <TextView
                android:id="@+id/tv_secondSkilllabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@{ttemSubscribeSecondSkilllabelModel.secondSkilllabelName}"
                android:textColor="@{ttemSubscribeSecondSkilllabelModel.secondSkilllabelColor}"
                android:textSize="14.5sp"/>

        </RelativeLayout>

    </LinearLayout>
</layout>

