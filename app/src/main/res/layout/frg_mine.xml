<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="viewModel"
            type="com.slash.youth.v2.feature.main.mine.MineViewModel" />

        <import type="android.view.View" />
    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="false">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#eaeaea">


            <com.slash.youth.ui.view.AlphaViewScrollView
                android:id="@+id/sv_pager_home_my"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="230dp">

                        <ImageView
                            android:id="@+id/iv_mine"
                            android:layout_width="match_parent"
                            android:layout_height="230dp"
                            android:scaleType="fitXY"
                            android:src="@mipmap/icon_mine_img"></ImageView>

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="76dp"
                            android:background="?attr/selectableItemBackground"
                            app:clickCommand="@{viewModel.personInfoClick}">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center_vertical"
                                android:orientation="vertical">

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="12dp"
                                        android:layout_marginRight="10dp"
                                        android:orientation="horizontal">

                                        <com.core.op.lib.weight.ShapedImageView
                                            android:id="@+id/iv_assistant_icon"
                                            android:layout_width="59dp"
                                            android:layout_height="59dp"
                                            android:layout_marginTop="2dp"
                                            android:scaleType="centerCrop"
                                            android:src="@mipmap/default_avatar"
                                            app:uri="@{viewModel.uri}" />

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginLeft="12dp"
                                            android:orientation="vertical">

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:orientation="horizontal">

                                                <TextView
                                                    android:id="@+id/tv_name"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="center_vertical"
                                                    android:text="@{viewModel.data.name}"
                                                    android:textColor="#333333"
                                                    android:textSize="16sp" />

                                                <ImageView
                                                    android:id="@+id/iv_v"
                                                    android:layout_width="14dp"
                                                    android:layout_height="14dp"
                                                    android:layout_gravity="center_vertical"
                                                    android:layout_marginLeft="9dp"
                                                    android:src="@mipmap/v_icon_home_my"
                                                    android:visibility="@{viewModel.data.isauth==1?View.VISIBLE:View.GONE}"
                                                    app:clickCommand="@{viewModel.identificateClick}" />

                                                <TextView
                                                    android:id="@+id/tv_my_Approval"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="center_vertical"
                                                    android:layout_marginLeft="9dp"
                                                    android:background="@drawable/shape_mine_identification_bg"
                                                    android:drawableLeft="@mipmap/exclamation"
                                                    android:drawablePadding="2dp"
                                                    android:paddingLeft="5dp"
                                                    android:paddingRight="5dp"
                                                    android:text="去认证"
                                                    android:textColor="@color/white"
                                                    android:textSize="12sp"
                                                    android:visibility="@{viewModel.data.isauth==0?View.VISIBLE:View.GONE}"
                                                    app:clickCommand="@{viewModel.approvalClick}" />

                                            </LinearLayout>

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="20dp"
                                                android:orientation="horizontal">

                                                <LinearLayout
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:gravity="center_horizontal"
                                                    android:orientation="vertical">

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:text="人脉"
                                                        android:textColor="#333333"
                                                        android:textSize="12sp" />

                                                    <TextView
                                                        android:id="@+id/tv_fans_count"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:text="@{viewModel.connection}"
                                                        android:textColor="#333333" />
                                                </LinearLayout>

                                                <View
                                                    android:layout_width="0.5dp"
                                                    android:layout_height="match_parent"
                                                    android:layout_marginLeft="10dp"
                                                    android:background="#adadad" />

                                                <LinearLayout
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:gravity="center_horizontal"
                                                    android:orientation="vertical">

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:text="顺利成交"
                                                        android:textColor="#333333"
                                                        android:textSize="12sp" />

                                                    <TextView
                                                        android:id="@+id/tv_my_Achieve_Task_Count"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:text="@{String.valueOf(viewModel.data.achievetaskcount)}"
                                                        android:textColor="#333333" />
                                                </LinearLayout>

                                                <View
                                                    android:layout_width="0.5dp"
                                                    android:layout_height="match_parent"
                                                    android:layout_marginLeft="10dp"
                                                    android:background="#adadad" />

                                                <LinearLayout
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:gravity="center_horizontal"
                                                    android:orientation="vertical">

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:text="服务力"
                                                        android:textColor="#333333"
                                                        android:textSize="12sp" />

                                                    <TextView
                                                        android:id="@+id/tv_service_point1"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:text="@{viewModel.serverStar}"
                                                        android:textColor="#333333" />
                                                </LinearLayout>
                                            </LinearLayout>
                                        </LinearLayout>
                                    </LinearLayout>


                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        android:layout_marginRight="10dp"
                                        android:src="@mipmap/icon_nav_back" />
                                </RelativeLayout>
                            </LinearLayout>

                        </FrameLayout>

                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:paddingBottom="15dp"
                        android:paddingTop="15dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="212dp"
                            android:background="@drawable/shape_rounded_my_news_center">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="5dp"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="19dp"
                                    android:layout_height="19dp"
                                    android:layout_gravity="center_vertical"
                                    android:src="@mipmap/expert_icon" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="5dp"
                                    android:text="服务力等级"
                                    android:textColor="#666666"
                                    android:textSize="14sp" />

                                <ImageView
                                    android:layout_width="15dp"
                                    android:layout_height="15dp"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="10dp"
                                    android:src="@mipmap/questionmark_icon"
                                    app:clickCommand="@{viewModel.influenceClick}" />

                            </LinearLayout>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="10dp"
                                android:layout_marginTop="8dp"
                                android:text="宗师与至尊级别需审核"
                                android:textColor="#CCCCCC"
                                android:textSize="11sp" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:layout_marginBottom="11dp"
                                android:layout_marginLeft="10dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom"
                                    android:textColor="#999999"
                                    android:textSize="11dp" />

                                <TextView
                                    android:id="@+id/tv_grade"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom"
                                    android:text="@{viewModel.grade}"
                                    android:textColor="#999999"
                                    android:textSize="11dp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom"
                                    android:textColor="#999999"
                                    android:textSize="11dp" />

                                <TextView
                                    android:id="@+id/tv_least_mark"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom"
                                    android:text="@{viewModel.mark}"
                                    android:textColor="#3CC7E3"
                                    android:textSize="19sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom"
                                    android:text="分 "
                                    android:textColor="#999999"
                                    android:textSize="11dp" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:layout_alignParentRight="true"
                                android:layout_marginBottom="11dp"
                                android:layout_marginRight="10dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom"
                                    android:text="超过 "
                                    android:textColor="#999999"
                                    android:textSize="11dp" />

                                <TextView
                                    android:id="@+id/tv_over"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom"
                                    android:text="@{viewModel.over}"
                                    android:textColor="#3CC7E3"
                                    android:textSize="19sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom"
                                    android:text="的用户 "
                                    android:textColor="#999999"
                                    android:textSize="11dp" />

                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="170dp"
                                android:layout_height="170dp"
                                android:layout_centerInParent="true">

                                <com.slash.youth.ui.view.RingScoreView
                                    android:id="@+id/rsv_home_my_expert_marks_progress"
                                    android:layout_width="135dp"
                                    android:layout_height="135dp"
                                    android:layout_centerInParent="true">

                                </com.slash.youth.ui.view.RingScoreView>

                                <RelativeLayout
                                    android:layout_width="135dp"
                                    android:layout_height="135dp"
                                    android:layout_centerInParent="true">

                                    <View
                                        android:layout_width="10dp"
                                        android:layout_height="4dp"
                                        android:layout_centerVertical="true"
                                        android:background="@drawable/shape_home_my_expert_marks_point" />

                                    <View
                                        android:layout_width="4dp"
                                        android:layout_height="10dp"
                                        android:layout_centerHorizontal="true"
                                        android:background="@drawable/shape_home_my_expert_marks_point" />

                                    <View
                                        android:layout_width="10dp"
                                        android:layout_height="4dp"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        android:background="@drawable/shape_home_my_expert_marks_point" />

                                    <View
                                        android:layout_width="4dp"
                                        android:layout_height="10dp"
                                        android:layout_alignParentBottom="true"
                                        android:layout_centerHorizontal="true"
                                        android:background="@drawable/shape_home_my_expert_marks_point" />

                                </RelativeLayout>

                                <FrameLayout
                                    android:layout_width="135dp"
                                    android:layout_height="135dp"
                                    android:layout_centerInParent="true">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="bottom|center_horizontal"
                                        android:src="@mipmap/rhythmic_bg" />

                                </FrameLayout>

                                <FrameLayout
                                    android:id="@+id/fl_home_my_expert_marks_maker"
                                    android:layout_width="140dp"
                                    android:layout_height="140dp"
                                    android:layout_centerInParent="true">

                                    <ImageView
                                        android:layout_width="15dp"
                                        android:layout_height="15dp"
                                        android:layout_gravity="center_horizontal"
                                        android:src="@mipmap/rhythmic_head" />
                                </FrameLayout>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:text="少侠"
                                    android:textColor="#CCCCCC"
                                    android:textSize="12sp" />

                                <TextView
                                    android:layout_width="11dp"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentRight="true"
                                    android:layout_centerVertical="true"
                                    android:gravity="center"
                                    android:text="大侠"
                                    android:textColor="#CCCCCC"
                                    android:textSize="12sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:layout_centerHorizontal="true"
                                    android:text="宗师"
                                    android:textColor="#CCCCCC"
                                    android:textSize="12sp" />

                                <TextView
                                    android:layout_width="11dp"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:gravity="center"
                                    android:text="至尊"
                                    android:textColor="#CCCCCC"
                                    android:textSize="12sp" />

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tv_home_my_expert_marks"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="0"
                                        android:textColor="#3DC7E3"
                                        android:textSize="24sp" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="分"
                                        android:textColor="#3DC7E3"
                                        android:textSize="17sp" />
                                </LinearLayout>

                            </RelativeLayout>


                        </RelativeLayout>

                    </FrameLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:background="@color/white"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:background="?attr/selectableItemBackground"
                            app:clickCommand="@{viewModel.accountClick}">

                            <ImageView
                                android:layout_width="23dp"
                                android:layout_height="21dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="10dp"
                                android:src="@mipmap/wallet_icon" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="8dp"
                                android:text="账户"
                                android:textColor="#718083"
                                android:textSize="12sp" />

                            <View
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_weight="1" />

                            <TextView
                                android:id="@+id/tv_amount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:text="@{viewModel.totalsMoney}"
                                android:textColor="#FF7333"
                                android:textSize="14sp" />

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="18dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="9dp"
                                android:layout_marginRight="10dp"
                                android:src="@mipmap/towardsright_icon" />

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp"
                            android:background="#eaeaea" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:background="?attr/selectableItemBackground"
                            app:clickCommand="@{viewModel.friendClick}">

                            <ImageView
                                android:layout_width="23dp"
                                android:layout_height="21dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="10dp"
                                android:src="@mipmap/icon_mine_friends" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="8dp"
                                android:text="好友"
                                android:textColor="#718083"
                                android:textSize="12sp" />

                            <View
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_weight="1" />

                            <ImageView
                                android:id="@+id/iv_mine_contacts"
                                android:layout_width="10dp"
                                android:layout_height="18dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="9dp"
                                android:layout_marginRight="10dp"
                                android:src="@mipmap/icon_mine_dot"
                                android:visibility="@{viewModel.contactsVisible}" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp"
                            android:background="#eaeaea" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:background="?attr/selectableItemBackground"
                            app:clickCommand="@{viewModel.visitorClick}">

                            <ImageView
                                android:layout_width="23dp"
                                android:layout_height="21dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="10dp"
                                android:src="@mipmap/icon_mine_recent" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="8dp"
                                android:text="最近访客"
                                android:textColor="#718083"
                                android:textSize="12sp" />

                            <View
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_weight="1" />

                            <ImageView
                                android:id="@+id/iv_mine_visitors"
                                android:layout_width="10dp"
                                android:layout_height="18dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="9dp"
                                android:layout_marginRight="10dp"
                                android:src="@mipmap/icon_mine_dot"
                                android:visibility="gone" />
                        </LinearLayout>
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginTop="15dp"
                        android:background="@color/white"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:background="?attr/selectableItemBackground"
                            app:clickCommand="@{viewModel.managerClick }">

                            <ImageView
                                android:layout_width="23dp"
                                android:layout_height="21dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="10dp"
                                android:src="@mipmap/icon_mine_task" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="8dp"
                                android:text="发布管理"
                                android:textColor="#718083"
                                android:textSize="12sp" />

                            <View
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_weight="1" />

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="18dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="9dp"
                                android:layout_marginRight="10dp"
                                android:src="@mipmap/towardsright_icon" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp"
                            android:background="#eaeaea" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:background="?attr/selectableItemBackground"
                            app:clickCommand="@{viewModel.collectionClick}">

                            <ImageView
                                android:layout_width="23dp"
                                android:layout_height="21dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="10dp"
                                android:src="@mipmap/icon_mine_collect" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="8dp"
                                android:text="收藏"
                                android:textColor="#718083"
                                android:textSize="12sp" />

                            <View
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_weight="1" />

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="18dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="9dp"
                                android:layout_marginRight="10dp"
                                android:src="@mipmap/towardsright_icon" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp"
                            android:background="#eaeaea" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:background="?attr/selectableItemBackground"
                            app:clickCommand="@{viewModel.helpClick}">

                            <ImageView
                                android:layout_width="23dp"
                                android:layout_height="21dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="10dp"
                                android:src="@mipmap/icon_mine_help" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="8dp"
                                android:text="帮助"
                                android:textColor="#718083"
                                android:textSize="12sp" />

                            <View
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_weight="1" />

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="18dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="9dp"
                                android:layout_marginRight="10dp"
                                android:src="@mipmap/towardsright_icon" />
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>
            </com.slash.youth.ui.view.AlphaViewScrollView>

            <FrameLayout
                android:id="@+id/rl_head"
                android:layout_width="match_parent"
                android:layout_height="68dp"
                android:background="#00000000"
                android:paddingTop="25dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="43dp">

                    <TextView
                        android:id="@+id/tv_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="我的"
                        android:textColor="#333333"
                        android:textSize="17sp" />

                    <ImageView
                        android:id="@+id/iv_edit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerInParent="true"
                        android:layout_marginRight="15dp"
                        android:background="?attr/selectableItemBackgroundBorderless"
                        android:src="@mipmap/icon_mine_editor"
                        app:clickCommand="@{viewModel.editorClick}" />

                    <ImageView
                        android:id="@+id/iv_set"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="30dp"
                        android:layout_toLeftOf="@id/iv_edit"
                        android:background="?attr/selectableItemBackgroundBorderless"
                        android:src="@mipmap/icon_mine_set"
                        app:clickCommand="@{viewModel.settingClick}" />

                </RelativeLayout>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/rl_head_up"
                android:layout_width="match_parent"
                android:layout_height="68dp"
                android:background="#31C5E4"
                android:paddingTop="25dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="43dp">

                    <TextView
                        android:id="@+id/tv_title_up"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="我的"
                        android:textColor="@color/white"
                        android:textSize="17sp" />

                    <ImageView
                        android:id="@+id/iv_edit_up"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerInParent="true"
                        android:layout_marginRight="15dp"
                        android:background="?attr/selectableItemBackgroundBorderless"
                        android:src="@mipmap/icon_mine_editor_up"
                        app:clickCommand="@{viewModel.editorClick}" />

                    <ImageView
                        android:id="@+id/iv_set_up"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="30dp"
                        android:layout_toLeftOf="@id/iv_edit_up"
                        android:background="?attr/selectableItemBackgroundBorderless"
                        android:src="@mipmap/icon_mine_set_up"
                        app:clickCommand="@{viewModel.settingClick}" />

                </RelativeLayout>
            </FrameLayout>

        </FrameLayout>
    </FrameLayout>
</layout>